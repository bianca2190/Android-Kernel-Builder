[
  {
    "kernelSource": {
      "name": "AvariceXC_Vayu",
      "repo": "https://github.com/bianca2190/platform_kernel_xiaomi_vayu",
      "branch": "staging/13.0",
      "device": "vayu",
      "defconfig": "vayu_defconfig"
    },
    "withKernelSU": true,
    "toolchains": [
      {
    "repo": "https://gitlab.com/LeCmnGend/proton-clang",
    "branch": "clang-16",
    "name": "neutron-clang",
    "binPath": ["bin"]
      }
    ],
    "ccache": false,
    "params": {
      "ARCH": "arm64",
      "CC": "$HOME/toolchains/neutron-clang/bin/clang",
      "externalCommand": {
        "CROSS_COMPILE": "$HOME/toolchains/neutron-clang/aarch64-linux-gnu-",
        "CROSS_COMPILE_ARM32": "$HOME/toolchains/neutron-clang/bin/arm-linux-gnueabi-",
        "LD": "$HOME/toolchains/neutron-clang/bin/ld.lld",
        "LLVM": "$HOME/toolchains/neutron-clang/bin/",
        "LLVM_IAS": "$HOME/toolchains/neutron-clang/bin/"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true,
      "repo": "https://github.com/bianca2190/AnyKernel3/",
      "branch": "master"
    }
  }
]
